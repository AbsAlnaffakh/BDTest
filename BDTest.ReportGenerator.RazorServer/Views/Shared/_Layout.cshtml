@{
    var title = ViewBag.Title ?? "BDTest Report";   
} 

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@title</title>
    @await Html.PartialAsync("_Css")
    @await Html.PartialAsync("_Javascript")
</head>
<body class="has-navbar-fixed-top">
@await Html.PartialAsync("_NavBarHeader")
@await Html.PartialAsync("_Tabs")
@RenderBody()
</body>
</html>