@model BDTest.NetCore.Razor.ReportMiddleware.Models.ViewModels.SetupTeardownOutputViewModel

@if (!string.IsNullOrEmpty(Model?.Content))
{
    <tr>
        <td class="has-text-centered is-vcentered">
            <details class="mr-5half">
                <summary class="button is-fullwidth">
                    @Model.Header
                </summary>
                
                <div class="container">
                    <article class="message is-link mt-4 has-text-left">
                        @* <div class="message-header"> *@
                        @*     <p>Output</p> *@
                        @* </div> *@
                        <pre class="message-body">@Model.Content.Trim()</pre>
                    </article>
                </div>
            </details>
        </td>
    </tr>
}